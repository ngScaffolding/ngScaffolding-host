{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-host": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "preserveSymlinks": true,
            "lazyModules": [

            ],
            "outputPath": "dist",
            "deleteOutputPath": false,
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/appConfig.json",
              "src/upload.php",
              "src/web.config",
              "src/modules/dcoSystems/fonts"
            ],
            "styles": [
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/fullcalendar/dist/fullcalendar.min.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/ag-grid/dist/styles/ag-grid.css",
              "node_modules/ag-grid/dist/styles/ag-theme-material.css",
              "src/styles.scss",
              "custom/theme/theme.scss"
            ],
            "scripts": [
              "node_modules/moment/moment.js",
              "node_modules/chart.js/dist/Chart.js",
              "node_modules/fullcalendar/dist/fullcalendar.js",
              "node_modules/quill/dist/quill.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-host:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-host:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-host:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "scripts": [
              "node_modules/moment/moment.js",
              "node_modules/chart.js/dist/Chart.js",
              "node_modules/fullcalendar/dist/fullcalendar.js",
              "node_modules/quill/dist/quill.js"
            ],
            "styles": [
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/fullcalendar/dist/fullcalendar.min.css",
              "node_modules/quill/dist/quill.snow.css",
              "styles.css"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/upload.php"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-host-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ng-host:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngscaffolding-core": {
      "root": "projects/ngscaffolding-core",
      "sourceRoot": "projects/ngscaffolding-core/src",
      "projectType": "library",
      "prefix": "ngs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngscaffolding-core/tsconfig.lib.json",
            "project": "projects/ngscaffolding-core/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/ngscaffolding-core/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngscaffolding-core/src/test.ts",
            "tsConfig": "projects/ngscaffolding-core/tsconfig.spec.json",
            "karmaConfig": "projects/ngscaffolding-core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngscaffolding-core/tsconfig.lib.json",
              "projects/ngscaffolding-core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngscaffolding-inputbuilder": {
      "root": "projects/ngscaffolding-inputbuilder",
      "sourceRoot": "projects/ngscaffolding-inputbuilder/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngscaffolding-inputbuilder/tsconfig.lib.json",
            "project": "projects/ngscaffolding-inputbuilder/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngscaffolding-inputbuilder/src/test.ts",
            "tsConfig": "projects/ngscaffolding-inputbuilder/tsconfig.spec.json",
            "karmaConfig": "projects/ngscaffolding-inputbuilder/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngscaffolding-inputbuilder/tsconfig.lib.json",
              "projects/ngscaffolding-inputbuilder/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngscaffolding-chart": {
      "root": "projects/ngscaffolding-chart",
      "sourceRoot": "projects/ngscaffolding-chart/src",
      "projectType": "library",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngscaffolding-chart/tsconfig.lib.json",
            "project": "projects/ngscaffolding-chart/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngscaffolding-chart/src/test.ts",
            "tsConfig": "projects/ngscaffolding-chart/tsconfig.spec.json",
            "karmaConfig": "projects/ngscaffolding-chart/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngscaffolding-chart/tsconfig.lib.json",
              "projects/ngscaffolding-chart/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngscaffolding-dashboard": {
      "root": "projects/ngscaffolding-dashboard",
      "sourceRoot": "projects/ngscaffolding-dashboard/src",
      "projectType": "library",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngscaffolding-dashboard/tsconfig.lib.json",
            "project": "projects/ngscaffolding-dashboard/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngscaffolding-dashboard/src/test.ts",
            "tsConfig": "projects/ngscaffolding-dashboard/tsconfig.spec.json",
            "karmaConfig": "projects/ngscaffolding-dashboard/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngscaffolding-dashboard/tsconfig.lib.json",
              "projects/ngscaffolding-dashboard/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngscaffolding-datagrid": {
      "root": "projects/ngscaffolding-datagrid",
      "sourceRoot": "projects/ngscaffolding-datagrid/src",
      "projectType": "library",
      "prefix": "ng",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngscaffolding-datagrid/tsconfig.lib.json",
            "project": "projects/ngscaffolding-datagrid/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngscaffolding-datagrid/src/test.ts",
            "tsConfig": "projects/ngscaffolding-datagrid/tsconfig.spec.json",
            "karmaConfig": "projects/ngscaffolding-datagrid/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngscaffolding-datagrid/tsconfig.lib.json",
              "projects/ngscaffolding-datagrid/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng-host",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "cli": {
    "defaultCollection": "akita-schematics"
  }
}
