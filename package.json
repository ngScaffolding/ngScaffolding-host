{
  "name": "ngscaffolding-host",
  "version": "1.0.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "libs": "rimraf dist & npm run lib-core & npm run lib-inputbuilder & npm run lib-chart & npm run lib-datagrid & npm run lib-dashboard",
    "lib-core": "ng build ngscaffolding-core & cd ./dist/ngscaffolding-core & npm pack",
    "lib-inputbuilder": "ng build ngscaffolding-inputbuilder & cd ./dist/ngscaffolding-inputbuilder & npm pack",
    "lib-chart": "ng build ngscaffolding-chart & cd ./dist/ngscaffolding-chart & npm pack",
    "lib-datagrid": "ng build ngscaffolding-datagrid & cd ./dist/ngscaffolding-datagrid & npm pack",
    "lib-dashboard": "ng build ngscaffolding-dashboard & cd ./dist/ngscaffolding-dashboard & npm pack",
    "extract-translate": "ngx-translate-extract --input ./src ./projects --output ./src/assets/i18n/ --clean --sort --format namespaced-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scss": "node-sass --source-map true custom/theme/theme.scss -o custom/theme",
    "version": "node version.js"
  },
  "private": true,
